<% if Rails.env.production? %>
  <% token = 'UA-113880451-1' %>

  <script async src="https://www.googletagmanager.com/gtag/js?id="<%= token %>></script>
  <script>
    window.dataLayer = window.dataLayer || [];
    function gtag(){dataLayer.push(arguments);}
    gtag('js', new Date());

    gtag('config', '<%= token %>');
  </script>
<% end %>
